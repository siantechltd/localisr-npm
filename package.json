{
  "name": "localisr",
  "version": "1.0.12",
  "description": "Localisr exists to make a developer's life a bit easier when developing multi-language websites and apps.",
  "main": "dist/cjs/index.js",
  "module": "dist/mjs/index.js",
  "exports": {
    ".": {
      "import": "./dist/mjs/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "scripts": {
    "fix": "eslint --fix src/**/*.ts",
    "build": "rm -fr dist/* && tsc -p tsconfig.json && tsc -p tsconfig-cjs.json && sudo ./fixup"
  },
  "keywords": [
    "multilanguage",
    "translations",
    "i18n",
    "internationalization",
    "localization",
    "business localization tools",
    "website localization services",
    "app localization services"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/siantechltd/localisr-npm.git"
  },
  "bugs": {
    "url": "https://localisr.dev/issues"
  },
  "author": "SiaNTech LTD",
  "license": "MIT",
  "homepage": "https://localisr.com",
  "dependencies": {
    "axios": "^1.4.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "benchmark": "^2.1.4",
    "eslint": "^8.42.0",
    "eslint-config-standard-with-typescript": "^35.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "typescript": "^5.1.3"
  }
}
